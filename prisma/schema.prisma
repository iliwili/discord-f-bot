datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Guild {
  id        String       @id
  name      String
  joined    DateTime  @default(now())
  user      User[]
}

model User {
  id        String       @id
  name      String
  fCount    Int
  guild     Guild     @relation(fields: [guildId], references: [id])
  guildId   String
}