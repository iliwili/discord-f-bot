datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Guild {
  id        String    @id
  name      String
  joined    DateTime  @default(now())
  init      Boolean   @default(false)
  users      User[]
}

model User {
  id        String    @id
  name      String
  tag       String
  fCount    Int       @default(0)
  guild     Guild     @relation(fields: [guildId], references: [id])
  guildId   String
}